{
  "manifest_version": 3,
  "name": "Text Replacement Tool",
  "version": "1.0",
  "description": "Replace text in a given format.",
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
